<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleEditTextView">
        <attr name="android:text" />
        <attr name="android:maxLength" />
        <attr name="android:inputType" />
        <attr name="android:enabled" />
        <attr name="android:gravity" />
        <attr name="android:imeOptions" />
        <attr name="title_text" format="string" />
        <attr name="nextFocus" format="reference" />
        <attr name="lines" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomTextInputLayout">
        <attr name="mainHintTextSize" format="dimension" />
    </declare-styleable>
</resources>